{
    "name": "roblox-ts-plugin",
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json"
    ],
    "tree": {
        "$className": "DataModel",
        "rorender/plugin": {
            "$className": "Folder",
            "$path": "out",
            "include": {
                "$path": "include",
                "node_modules": {
                    "$className": "Folder",
                    "@rbxts": {
                        "$path": "node_modules/@rbxts"
                    }
                }
            }
        }
    }
}
